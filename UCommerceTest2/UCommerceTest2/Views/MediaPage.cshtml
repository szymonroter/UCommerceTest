@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.MediaPage>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
}

<div class="row">
    <img src="@Umbraco.TypedMedia(CurrentPage.image).Url" class="mediapage-image"/>
</div>

<span class="image-desc"> Description: @Umbraco.TypedMedia(CurrentPage.image).Description</span>
